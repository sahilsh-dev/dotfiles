{
    "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace",
    "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
    "editor.fontLigatures": true,
    "keyboard.dispatch": "keyCode",
    "editor.fontSize": 16,
    "window.zoomLevel": 1.5,
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false,
        "javascript": true,
        "python": true,
        "javascriptreact": true,
        "go": true,
        "typescript": true,
        "ignore": true,
        "shellscript": false
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.formatOnSave": false
    },
    "editor.rulers": [
        80,
    ],
    "prettier.useTabs": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "notebook.output.scrolling": true,
    "jupyter.askForKernelRestart": false,
    "vim.smartRelativeLine": true,
    "editor.linkedEditing": true,
    "workbench.colorTheme": "Ros√© Pine (no italics)",
    "editor.minimap.enabled": false,
    "yaml.schemas": {
        "file:///home/sahil/.vscode/extensions/atlassian.atlascode-3.8.3/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "redhat.telemetry.enabled": true,
    "editor.renderWhitespace": "none",
    "window.menuBarVisibility": "toggle",
    "atlascode.jira.enabled": false,
    "atlascode.helpExplorerEnabled": false,
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "editor.action.goToReferences"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": ["leader", "f"],
            "commands": ["editor.action.formatSelection"]
        }
    ],
    "python.defaultInterpreterPath": "/bin/python",
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "git.openRepositoryInParentFolders": "never",
    "github.copilot.nextEditSuggestions.enabled": false,
    "workbench.settings.applyToAllProfiles": [],
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "ruff.lint.enable": false,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "chat.editing.confirmEditRequestRetry": false,
    "git.confirmSync": false,
    "docker.extension.enableComposeLanguageServer": false,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
}
